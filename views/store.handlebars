<!doctype html>

<html>

	<head>
		<title>Store</title>
		<link href="css/PocketPup.css" rel="stylesheet">
		<link href="css/bootstrap.css" rel="stylesheet">
		<link href="css/bootstrap-theme.css" rel="stylesheet">
		<link href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.6.3/css/font-awesome.min.css" rel="stylesheet" >
		<meta name="viewport" content="width=device-width, initial-scale=1.0">
	</head>


	<body>

		<div class="bar top">
			<a href="activity" id="actButton"><i class="fa fa-bar-chart"></i></a>
			<p class="pageTitle">Store</p>
			<a href="store" class="active top bones" id="storeButton">{{cashmonkey}} <i class="fa fa-credit-card"></i></a>
		</div>

		<div class="buffer"></div>

		<div class="storeMenu">
			<p class="" id="indoor"><i class="fa fa-home"></i></p>
			<p class="" id="outdoor"><i class="fa fa-tree"></i></p>
			<p class="" id="furniture"><i class="fa fa-tv"></i></p>
			<p class="" id="accessories"><i class="fa fa-diamond"></i></p>
			<p class="" id="toys"><i class="fa fa-futbol-o"></i></p>
		</div>

		<div class="storeContent"> 
			<div class="storeLevel" id="topLevel">
				<p class="itemIcon" id="iconOne"><img src="{{image1}}" height="120" width="auto"></p>
				<p class="itemIcon" id="iconTwo"><img src="{{image2}}" height="120" width="auto"></p>
			</div>
			<div class="storeLevel" id="midLevel">
				<p class="itemIcon" id="iconThree"><img src="{{image3}}" height="120" width="auto"></p>
				<p class="itemIcon" id="iconFour"><img src="{{image4}}" height="120" width="auto"></p>
			</div>
			<div class="storeLevel" id="bottomLevel">
				<p class="itemIcon" id="iconFive"><img src="{{image5}}" height="120" width="auto"></p>
				<p class="itemIcon" id="iconSix"><img src="{{image6}}" height="120" width="auto"></p>
			</div>
		</div>

		<!-- <div id="arrowSpace">
			<p id="leftArrow"><img src="images/store/left_arrow.png" width="50" height="auto"></p>
			<p id="rightArrow"><img src="images/store/right_arrow.png" width="50" height="auto"></p>
		</div> -->

		<div class="buffer"></div>


		<div class="bar bottom">
 			<a href="home"><i class="fa fa-home"></i></a> 
  			<a href="locations"><i class="fa fa-map-marker"></i></a> 
 			<a href="training"><i class="fa fa-paw"></i></a> 
		  <a href="vote"><i class="fa fa-check-square-o"></i></a>
		</div>

	</body>
